# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET /users   com.thoughtworks.vodqa.Controller.VodQAController.listUsers(employer: Option[String],location: Option[String])
GET /users/:id   com.thoughtworks.vodqa.Controller.VodQAController.getUser(id: Int)
POST /users   com.thoughtworks.vodqa.Controller.VodQAController.addUsers()
PUT /users com.thoughtworks.vodqa.Controller.VodQAController.putUser()
PUT /users/:id com.thoughtworks.vodqa.Controller.VodQAController.updateUser(id: Int)
DELETE /users/:id com.thoughtworks.vodqa.Controller.VodQAController.deleteUser(id: Int)

